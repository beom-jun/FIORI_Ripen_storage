{
  "_version": "1.60.0",
  "sap.app": {
    "id": "ripeninig.ripeninig",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.1",
      "toolsId": "d6351e16-48d9-4be0-8a81-a2a3a3847708"
    },
    "dataSources": {
      "ZC102_PP_RIP01_CDS_VAN": {
        "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='ZC102_PP_RIP01_CDS_VAN',Version='0001')/$value/",
        "type": "ODataAnnotation",
        "settings": {
          "localUri": "localService/mainService/ZC102_PP_RIP01_CDS_VAN.xml"
        }
      },
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZC102_PP_RIP01_CDS/",
        "type": "OData",
        "settings": {
          "annotations": [
            "ZC102_PP_RIP01_CDS_VAN"
          ],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.120.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ripeninig.ripeninig.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "ripeninig.ripeninig.view",
        "async": true,
        "viewPath": "ripeninig.ripeninig.view"
      },
      "routes": [
        {
          "name": "RouteRIP_VIEW",
          "pattern": ":?query:",
          "target": [
            "TargetRIP_VIEW"
          ]
        }
      ],
      "targets": {
        "TargetRIP_VIEW": {
          "id": "RIP_VIEW",
          "name": "RIP_VIEW"
        }
      }
    },
    "rootView": {
      "viewName": "ripeninig.ripeninig.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
